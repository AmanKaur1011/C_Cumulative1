<!-- This view shows the data about the selected  student-->
@model C_Cumulative1.Models.Student
@{
    ViewBag.Title = "Show";
}

<h2>Show</h2>
<div><strong>Student Id: </strong>@Model.StudentId</div>
<div>
    <strong>Student Name: </strong>@Model.StudentFname @Model.StudentLname
</div>
<div> <strong>Student Number: </strong> @Model.StudentNumber</div>
<div> <strong>Enrol Date: </strong>@Model.EnrolDate</div>


